function [] = plotTemporalModes(v)
%PLOTTEMPORALMODESL Summary of this function goes here
%   Detailed explanation goes here

% project first three modes for pulse and no pulse labels
plot3(v(1:n_pulse,1),v(1:n_pulse,2),v(1:n_pulse,3),'o','MarkerEdgeColor','k',...
'MarkerFaceColor',[156 240 9]/255 ,'MarkerSize',8), grid on
hold on

plot3(v2(n_pulse+1:end,1),v2(n_pulse+1:end,2),v(n_pulse+1:end,3),'o','MarkerEdgeColor','k',...
'MarkerFaceColor','b','MarkerSize',8), grid on
     

%Format figure
axis([-.1 .1 -.1 .1 -.1 .1]);
xlabel(['Mode ' num2str(j)])
ylabel(['Mode ' num2str(j+1)])
zlabel(['Mode ' num2str(j+2)])
legend('pulse', 'no Pulse')

end

